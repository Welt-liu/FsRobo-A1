<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="are1018_urdf">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import are1018_urdf urdf file -->
    <xacro:include filename="$(find are1018_urdf)/urdf/arm_urdf_1018.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="are1018_urdf.ros2_control.xacro" />


    <xacro:are1018_urdf_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
